mixin roadmap(data)
    .roadmap
        .roadmap__title Roadmap 
        .roadmap__content
            each val in roadmap.list
                .roadmap__items
                    .roadmap__year #{val.year}
                    .roadmap__quartal #{val.title}
                    each item in val.steps
                        .roadmap__item
                            if item.checked
                                img(src='%=static=%img/assets/roadmap/checked.svg').roadmap__item-icon
                            else
                                img(src='%=static=%img/assets/roadmap/void.svg').roadmap__item-icon
                            div(class=item.checked ? 'roadmap__item-text' : 'roadmap__item-text _unchecked') #{item.label}